{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "UnfoldedCircle.Server": "Trace",
      "UnfoldedCircle.OppoBluRay": "Trace",
      "Oppo": "Trace",
      "Makaretu.Dns": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffK ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
